<?xml version="1.0" encoding="UTF-8"?>
<JdbcEntitySetup
	xmlns="http://www.ontimize.com/schema/jdbc"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.ontimize.com/schema/jdbc http://www.ontimize.com/schema/jdbc/ontimize-jdbc-dao.xsd"
	catalog="" schema="${mainschema}" table="disc_song"
	datasource="mainDataSource" sqlhandler="dbSQLStatementHandler">
	<DeleteKeys>
		<Column>id_disc_song</Column>
	</DeleteKeys>
	<UpdateKeys>
		<Column>id_disc_song</Column>
	</UpdateKeys>
	<GeneratedKey>id_disc_song</GeneratedKey>
	<Queries>
		<Query id="default">
			<AmbiguousColumns>
		    	<AmbiguousColumn name="ID_SONG" prefix="DS" />		   	
		    	<AmbiguousColumn name="ID_DISC" prefix="D" />		   	
		    	<AmbiguousColumn name="ID_ARTIST" prefix="A" />
		   	</AmbiguousColumns>
			<Sentence>
    			<![CDATA[
    				SELECT
    					#COLUMNS#
    				FROM
    					PUBLIC.SONG S
    					INNER JOIN PUBLIC.DISC_SONG DS ON
    					S.ID_SONG = DS.ID_SONG
    					INNER JOIN PUBLIC.DISC D ON
    					DS.ID_DISC = D.ID_DISC
    					INNER JOIN PUBLIC.ARTIST A ON
    					D.ID_ARTIST = A.ID_ARTIST    							     	
    				#WHERE#		
    			]]>
    		</Sentence>
		</Query>
	</Queries>
</JdbcEntitySetup>